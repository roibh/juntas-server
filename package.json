  "scripts": {
    "docker": "docker image build -t juntasonline/juntas-server:1.3 .",
    "heroku": "npm version patch && git push heroku --force bitbucket:master",
    "build": "tsc",
    "watch": "tsc -w && npm start",
    "start": "node master.js",
    "test": "nyc --reporter=lcov --reporter=html npm run xtest && nyc report",
    "xtest": "npm run build && alsatian tests/*.spec.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "test-unit": "mocha tests --recursive --reporter spec"
  },
