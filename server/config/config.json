{
    "name": "testing all node options",
    "url": "",
    "port": "3030",
    "proport": "80",
    "appRoot": "/",
    "defaultPage": "public/index.html",
    "database": {
        "mongodb": {
            "useObjectId": true,
            "host": "mongodb://juntas:juntas$sa@ds049624.mongolab.com:49624/juntas"
        }
    },
    "plugins": [
        "webrtc",
        "user",
        "tabs"
    ],
    "mimeTypes": {
        ".html": "text/html",
        ".css": "text/css",
        ".js": "text/javascript",
        ".less": "text/less",
        ".json": "application/json",
        ".ttf": "application/font-woff",
        ".woff": "application/font-woff"
    },
    "logs": {
        "transports": [
            {
                "type": "winston.transports.File",
                "name": "info-file",
                "filename": "./logs/filelog-info.log",
                "level": "info"
            },
            {
                "type": "winston.transports.File",
                "name": "error-file",
                "filename": "./logs/filelog-error.log",
                "level": "error"
            },
            {
                "type": "winston.transports.File",
                "name": "warn-file",
                "filename": "./logs/filelog-warn.log",
                "level": "warn"
            },
            {
                "type": "winston.transports.Console",
                "name": "debug-file",
                "level": "debug"
            }
        ]
    }
}